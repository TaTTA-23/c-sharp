/*
exemplo de uso de foreach -  maneira simples e limpa de iterar atraves dos elementos de uma matriz em ordem de indice crescente.
*/


using System;

public class Program
{
    public static void Main(string[] args)
    {
        //Criando uma instância da classe PedidosIDs
        PedidosOrderIDs pedidosOrder = new PedidosOrderIDs();
       
    }
}

 public  class PedidosOrderIDs
{
    //Declaraçao do array
    string[]  pedidosOrderIDs;
    
    // Construtor da classe
    public  PedidosOrderIDs()
    {
        //Inicialização do array dentro do construtor
        pedidosOrderIDs = new string[]  {"Rowena", "Robin", "Bao"};
        
        foreach (string pedido in pedidosOrderIDs)
        {
            Console.WriteLine(pedido);
        }
    }
}
